{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "node": "current"
      },
      "useBuiltIns": true
    }],
    "react",
    "es2015",
    "flow"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    "transform-react-display-name",
    "transform-class-properties"
  ]
}
